                                else if (@Model.TransactionCategory == "BankContra")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>

                                }
                                else if (@Model.TransactionCategory == null && @Model.TransactionType == "Increase")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>

                                }
                                else
                                {
                                    <td></td>
                                    <td></td>
                                }
                                </tr>
                            }
              
                        }
                        if (Model.AccountAssetLiability != null)
                        {
                            <tr style="font-family:Arial Narrow;font-size:16px;">
                                <td>@Html.DisplayFor(modelItem => modelItem.AccountAssetLiability.AccCode) </td>
                                <td>
                                    @Model.AccountAssetLiability.AccName


                                    @if (Model.Sales != null)
                                    {
                                        @if (Model.Sales.CustomerModel.Name.ToUpper().Contains("CUSTOMER"))
                                        {
                                            <span class="">-</span>
                                            @Model.Sales.CustomerName
                                            ;
                                        }
                                        else //if (Model.Sales.CustomerModel != null )
                                        {
                                            <span class="">-</span>
                                            @Model.Sales.CustomerModel.Name ;
                                        }
                                    }
                                    @*
                                        @Html.DisplayFor(modelItem => modelItem.AccountAssetLiability.AccName)
                                    *@

                                </td>
                                @if (Model.TransactionCategory == "Received" || Model.TransactionCategory == "Paid")
                                {
                                    <td></td>
                                    <td></td>
                                }
                                else if (@Model.TransactionCategory == "Received" || @Model.TransactionCategory == "Supplier")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount + @Model.DiscountAmount))) </td>
                                    <td></td>

                                }
                                else if (@Model.TransactionType == "Decrease" || @Model.TransactionCategory == "Paid" || @Model.TransactionCategory == "Customer" || @Model.TransactionCategory == "CustomerDiscount" || @Model.TransactionCategory == "Sales")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount+ @Model.DiscountAmount))) </td>
                                }
                                else if (@Model.TransactionCategory == "Contra" && @Model.TransactionType == "Received")
                                {
                                    
                                    <td></td>
                                     <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                   
                                }
                                 else if (@Model.TransactionCategory == "Journal" && @Model.TransactionType == "Received")
                                {
                                    
                                    <td></td>
                                     <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                   
                                }
                                else if (@Model.TransactionCategory == null && @Model.TransactionType == "Increase")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount+ @Model.DiscountAmount))) </td>


                                }
                                else if (@Model.TransactionCategory == null && @Model.TransactionType == "Received")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount+ @Model.DiscountAmount))) </td>


                                }

                                else
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>

                                }



                            </tr>
                        }



                    }
                </tbody>
                <tfoot>
                    <tr class="text-right">
                        <td colspan="2" class="bg-asswhite font-weight-bold"><h6>Total :</h6></td>
                        <td><h6>@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount + @Model.DiscountAmount)))</h6></td>
                        <td><h6>@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount+ @Model.DiscountAmount)))</h6></td>
                    </tr>
                    <tr>
                        <td colspan="4">
                            <h6>In Words (Net Payable) : @clsproc.GTRInwordsFormatBD((@Model.TransactionAmount + @Model.DiscountAmount).ToString(), "", "");</h6>
                            @if (Model.isPost == true)
                            {
                                <p class="float-right">
                                    Posted
                                </p>

                            }


                        </td>
                    </tr>
                    <tr>
                        <td colspan="4">
                            <p>Narration : @Model.Description</p>

                            @if (Model.SalesId != null)
                            {
                                <a href="#">
                                            @Html.ActionLink(@Model.Sales.SaleCode + "","InvoiceViewReport" , "Sales" , new { SaleId = @Model.Sales.Id , ReportType = "Sales Invoice" }, new { @target = "_blank" , title="Go To The Link"})
                                </a>
                            
                            }




                


                            @foreach (var item in @Model.TransactionDetails)
                            {
                                if (item.SalesId != null)
                                {
                                    <p>@item.Sales.SaleCode - Bill Amount : @item.Sales.NetAmount ;  Received Amount : @item.NetAmount ; Discount : @item.Discount</p>
                                }
                                else if (item.PurchaseId != null)
                                {
                                     <p>@item.Purchase.PurchaseCode - Bill Amount : @item.Purchase.NetAmount ;  Paid Amount : @item.NetAmount ; Discount : @item.Discount</p>
                                   
                                }
                            }

                        </td>
                    </tr>
                </tfoot>
            </table>

                            
            @if (IsSignature == 1)
            {
                <div class="row mt-5 pt-4 text-center">
                    <div class="col-md-4">
                        <h4 class=" border-top border-dark">Prepared By</h4>
                    </div>
                    <div class="col-md-4">
                        <h4 class=" border-top border-dark">Checked By</h4>
                    </div>
                    <div class="col-md-4">
                        <h4 class=" border-top border-dark">Approved By</h4>
                    </div>

                </div>


                <div class="row  my-5 pt-5 text-center">
                    <div class="col-md-4">
                    <h4 class="border-top border-dark"> Received By </h4>
                </div>
                <div class="col-md-8"></div>

            </div>
