                        }
            </div>
                            <hr class="m-0" />


            <div class="row mb-2">
                <div class="col-md-12 text-center">
                    @{

                        if (@Model.TransactionCategory == "Received" || @Model.TransactionCategory == "Customer" || @Model.TransactionCategory == "Sales")
                        {
                            <h3><span class="border-bottom border-dark">Receipt Voucher</span></h3>
                        }
                        else if (@Model.TransactionCategory == "Paid" || @Model.TransactionCategory == "Supplier" || @Model.TransactionCategory == "Purchase")
                        {
                            <h3><span class="border-bottom border-dark">Payment Voucher</span></h3>
                        }
                        else if (@Model.TransactionCategory == "CashContra" || @Model.TransactionCategory == "BankContra" || @Model.TransactionCategory == "Contra")
                        {
                            <h3><span class="border-bottom border-dark">Contra Voucher</span></h3>
                        }
                        else if (@Model.TransactionCategory == null || @Model.TransactionCategory == "CustomerDiscount" || @Model.TransactionCategory == "SupplierDiscount")
                        {
                            <h3><span class="border-bottom border-dark">Journal Voucher</span></h3>
                        }
                        else
                        {
                            <h3><span class="border-bottom border-dark"> @Model.TransactionCategory</span></h3>
                        }

                    }


                </div>


            </div>

            @if (@Model.Customer != null && @Model.TransactionAmount != null)
            {
                <div class="col-md-10 offset-md-1 mb-3">
                    <div class="row text-center bg-offwhite">
                        <div class="col-md-6">
                            <h5><span class="border-bottom border-dark">Receipient</span></h5>
                        </div>
                        <div class="col-md-6">
                            <h5><span class="border-bottom border-dark">Amount</span></h5>
                        </div>
                    </div>
                    <div class="row text-center">
                        <div class="col-md-6">
                            <h6>@Model.Customer.Name</h6>
                        </div>
                        <div class="col-md-6">
                            <h6>@Model.TransactionAmount</h6>
                        </div>
                    </div>
                </div>
            }
            <div class="col-md-12 mb-2">
                <div class="row font-weight-bold text-center">
