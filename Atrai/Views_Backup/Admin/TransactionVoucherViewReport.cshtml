                                else if (@Model.TransactionCategory == "Paid" || Model.TransactionCategory == "Supplier" || @Model.TransactionCategory == "CustomerDiscount")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
