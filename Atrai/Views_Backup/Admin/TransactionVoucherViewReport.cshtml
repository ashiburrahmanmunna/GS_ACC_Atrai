                                        {
                                            <span class="">-</span>
                                        }
                                    }
                                </td>
                                @if (Model.TransactionCategory == "Received" || Model.TransactionCategory == "Customer" || @Model.TransactionType == "Received")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>
                                }
                                else if (@Model.TransactionCategory == "Paid" || @Model.TransactionCategory == "Supplier" || @Model.TransactionType == "Paid")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                }
                                else if (@Model.TransactionCategory == "CashContra")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>
                                }
                                else if (@Model.TransactionCategory == "BankContra")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                }
                                else if (@Model.TransactionCategory == "Contra" && @Model.TransactionType == "Received")
                                {
                                     <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>
                                   
                                }
                                else if (@Model.TransactionCategory == null && @Model.TransactionType == "Increase")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.TransactionAmount))) </td>
                                    <td></td>

                                }
                                else
                                {
                                    <td></td>
                                    <td></td>
                                }
                            </tr>

                            if (Model.DiscountAmount > 0)
                            {
                                    <tr style="font-family:Arial Narrow;font-size:16px;">
                                <td>@Html.DisplayFor(modelItem => modelItem.AccountPayType.AccCode) </td>
                                <td>

                                    Discount Expense

                                    @if (Model.Sales != null)
                                    {
                                        @if (Model.Sales.CustomerModel.Name.ToUpper().Contains("CUSTOMER"))
                                        {
                                            <span class="">-</span>
                                            @Model.Sales.CustomerName
                                            ;
                                        }
                                        else //if (Model.Sales.CustomerModel != null )
                                        {
                                            <span class="">-</span>
                                        }
                                    }
                                </td>
                                @if (Model.TransactionCategory == "Received" || Model.TransactionCategory == "Customer" || @Model.TransactionType == "Received")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.DiscountAmount))) </td>
                                    <td></td>
                                }
                                else if (@Model.TransactionCategory == "Paid" || @Model.TransactionCategory == "Supplier" || @Model.TransactionType == "Paid")
                                {
                                    <td></td>
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.DiscountAmount))) </td>
                                }
                                else if (@Model.TransactionCategory == null && @Model.TransactionType == "Increase")
                                {
                                    <td class="text-right">@(String.Format(BDFormat, "{0:N}", Convert.ToDecimal(@Model.DiscountAmount))) </td>
                                    <td></td>

                                }
                                else
                                {
                                    <td></td>
                                    <td></td>
                                }
                            </tr>
                            }
                        }
                        if (Model.Account != null)
                        {
                            if (Model.TransactionAmount > 0)
                            {
                            <tr style="font-family:Arial Narrow;font-size:16px;">
                                <td>@Html.DisplayFor(modelItem => modelItem.Account.AccCode) </td>
                                <td>
                                    @Model.Account.AccName

                                    @if (Model.Sales != null)
                                    {
                                        @if (Model.Sales.CustomerModel.Name.ToUpper().Contains("CUSTOMER"))
                                        {
                                            <span class="">-</span>
                                            @Model.Sales.CustomerName
                                            ;
                                        }
                                        else //if (Model.Sales.CustomerModel != null )
                                        {
                                            <span class="">-</span>
                                            @Model.Sales.CustomerModel.Name;
                                        }
                                    }

