@using Microsoft.AspNetCore.Http
@using Atrai.Core.Common
@inject IHttpContextAccessor HttpContextAccessor
@inject clsProcedure clsproc
@model Atrai.Core.Entity.TransactionModel
@{
    ViewData["Title"] = "Transaction Voucher";
    Layout = "~/Views/Shared/_PrintLayout.cshtml";
    
    var comimagepath = @HttpContextAccessor.HttpContext.Session.GetString("comImagePath");
    var IsMobile =@HttpContextAccessor.HttpContext.Session.GetInt32("IsMobile");
    var IsLogo = 0;
    int IsSignature = 0;
    if (Model.CompanyList.storeinfo.FirstOrDefault().IsSignature == true)
    {
         IsSignature = 1; 
    }

    if (Model.CompanyList.storeinfo.FirstOrDefault().SalesReportStyle != null)
    {
        if (Model.CompanyList.storeinfo.FirstOrDefault().SalesReportStyle.LogoPlacement == "LeftSide")
        {
            IsLogo = 1;
        }
        else if (Model.CompanyList.storeinfo.FirstOrDefault().SalesReportStyle.LogoPlacement == "RightSide")
        {
            IsLogo = 2;
        }
        else
        {
            IsLogo = 0;
            
            
        }
    }



    System.Globalization.CultureInfo BDFormat = new System.Globalization.CultureInfo("bn-BD");
    if (Model.Currency != null)
    {
        var invoicecultureformat = Model.Currency.CultureInfo;
        @if (invoicecultureformat != null)
        {
            BDFormat = new System.Globalization.CultureInfo(invoicecultureformat.ToString());
        }
    }
}

