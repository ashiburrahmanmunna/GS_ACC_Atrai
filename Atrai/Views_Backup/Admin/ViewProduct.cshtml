                let producttype = $("#ProductType").val();
                let issystem = $("#isSystem").val();
                //alert(issystem);

                //alert(producttype);
                    $("#divParentProduct").hide();
              
                    //$("#divMultiColor").hide();



                if (producttype == "C") {
                    $("#divParentProduct").hide();
                    $("#divComplexItem").show();
                    //$("#divMultiColor").show();


                    
                }
                else if (producttype == "S")
                {



                    if (issystem == "true") 
                    { 
                        $("#divParentProduct").show();
                    }
