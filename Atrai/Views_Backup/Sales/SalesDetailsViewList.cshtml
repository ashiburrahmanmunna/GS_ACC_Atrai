        function GetDate(str) {
            var arr = str.split("-");
            var months = ["jan", "feb", "mar", "apr", "may", "jun", "jul", "aug", "sep", "oct", "nov", "dec"];

            var month = months.indexOf(arr[1].toLowerCase());

            return new Date(parseInt(arr[2]), month, parseInt(arr[0]));
        }




