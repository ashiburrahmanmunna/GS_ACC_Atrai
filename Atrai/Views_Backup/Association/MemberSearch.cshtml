@model Atrai.Core.Entity.MemberModel
@{ ViewData["Title"] = "Member Search";
    Layout = "~/Views/Shared/_PrintLayout.cshtml"; }



<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>
    </title>
    <script type="text/javascript">
        function ClosePrint() {
            var print = document.getElementById("print");
            print.style.visibility = "hidden";
            //            print.display = false;

            window.print();
        }
    </script>

    <style media="print">
        .showHeader thead {
            display: table-header-group;
            border: 1px solid #000;
        }
    </style>

    <style type="text/css">
        .bg-offwhite {
            background-color: #D3D3D3;
        }

        .table-bordered td, .table-bordered th, .table-bordered {
            border: 1px solid black;
            color: black;
        }

        .table thead th {
            /*border-bottom: inherit;*/
        }

        .table th {
            /* background: inherit;*/
        }

        .bg-asswhite {
            background-color: #F5F5F5 !important;
        }
        /*#main {
            width: 620px;*/
        /* to centre page on screen*/
        /*margin-left: auto;
            margin-right: auto;
        }

        @@font-face {
            font-family: "Dodgv2";
            src: url("../../MenuCssJs/fonts/Dodgv2.ttf")format("truetype");
            font-weight: normal;
            font-style: normal;
        }*/

        /*        #btnPrint {
            font-weight: 700;
            font-style: italic;
        }*/

        .style8 {
            text-align: left;
            font-family: Calibri;
            font-size: 16px;
            width: 849px;
        }

        .style9 {
            font-size: 14px;
            text-align: left;
            width: 60%;
        }

        .style13 {
            width: 2px;
            font-weight: 700;
        }

        .style16 {
            width: 815px;
            font-weight: bold;
            font-family: Calibri;
