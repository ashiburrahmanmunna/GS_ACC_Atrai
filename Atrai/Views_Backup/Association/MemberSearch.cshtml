        }
    </style>

</head>
<body>
    <form asp-controller="Association" asp-action="MemberSearch" method="get">
        <div id="main">

            <div class="container" style="align-content:center;">

                <!-- Start Page Content -->




                <br />
                <div class="card p-2 mb-2">
                    <div class="row">

                        <div class="col-md-6 col-12">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text py-0">
                                        <label for="Fbarcode">Member Barcode</label>
                                    </span>
                                </div>
                                @Html.TextBox("MemberBarcode", null, new { @class = "form-control text-center MemberBarcode", @placeholder = "Enter Member Barcode ", @autocomplete = "off", name = "MemberBarcode" })
                            </div>
                        </div>
                        <div class="col-md-2 col-12">

                            <button type="submit" class="btn btn-success btn-block  rounded-0"> Search </button>

                            @*@Html.ActionLink("Search", "MemberSearch", "Association", null, new { @class = "btn btn-success btn-block rounded-0", @id = "btnBack" })*@
                            @*@Html.ActionLink("Search", "MemberSearch", "Association", null, new { MemberBarcode = " document.getElementById('MemberBarcode').value; ", @class = "btn btn-success btn-block rounded-0", @id = "btnBack" })*@

                            @*@Html.ActionLink("Search", "MemberSearch", "Association", null, new { onclick = "this.href += '&MemberSearch=' + document.getElementById('MemberSearch').value;" })*@
                        </div>

                    </div>
                </div>
                <br />


                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header py-1">
                                <h4>Member Information</h4>

                            </div>
                            <div id="target" class="card-body">
                                <div class="basic-elements">


                                    <div class="row">
                                        <div class="col-lg-8">



                                            <div class="form-group">
                                                <div class="col-md-5 col-md-offset-2">
                                                    @if (@Model != null)
                                                    {
                                        <img src="..@Model.MemberImagePath" alt="img" id="UploadFile" class="img-thumbnail" style="width:400px" /> <br /> } @*<input type="file" name="logoPostedFileBase" accept="image/x-png, image/gif, image/jpeg" id="FileUpload" onchange="PreviewImage()" />*@
                                                </div>
                                            </div>



                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="MemberAccessId"></label>
                                                        <input type="text" asp-for="MemberAccessId" class="form-control" />
                                                        <span asp-validation-for="MemberAccessId" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label for="MemberType"> Member Type : </label>
                                                        @Html.DropDownList("MemberType", new List<SelectListItem>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    {
                                                        new SelectListItem() {Text = "Life Member", Value="Life Member"},
                                                        new SelectListItem() {Text = "General Member", Value="General Member"},  },
                                                                                                                                                                                                                       "Select Member Type", new { @class = "form-control" })
                                                    </div>

                                                </div>
                                            </div>


                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="MembersNameEng"></label>
                                                        <input type="text" asp-for="MembersNameEng" class="form-control" />
                                                        <span asp-validation-for="MembersNameEng" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="MembersNameBng"></label>
                                                        <input type="text" asp-for="MembersNameBng" class="form-control" />
                                                        <span asp-validation-for="MembersNameBng" class="text-danger"></span>
                                                    </div>

                                                </div>
                                            </div>


                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="FathersNameEng"></label>
                                                        <input asp-for="FathersNameEng" class="form-control" type="text" />
                                                        <span asp-validation-for="FathersNameEng" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="FathersNameBng"></label>
                                                        <input asp-for="FathersNameBng" class="form-control" type="text" />
                                                        <span asp-validation-for="FathersNameBng" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="ShopNameEng"></label>
                                                        <input asp-for="ShopNameEng" class="form-control" type="text" />
                                                        <span asp-validation-for="ShopNameEng" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="ShopNameBng"></label>
                                                        <input asp-for="ShopNameBng" class="form-control" type="text" />
                                                        <span asp-validation-for="ShopNameBng" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="DOB"></label>
                                                        <input asp-for="DOB" class="form-control" type="text" />
                                                        <span asp-validation-for="DOB" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="BloodGroup"></label>
                                                        <input asp-for="BloodGroup" class="form-control" type="text" />
                                                        <span asp-validation-for="BloodGroup" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>



                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="Mobile"></label>
                                                        <input asp-for="Mobile" class="form-control" type="text" />
                                                        <span asp-validation-for="Mobile" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="MemberHomePhone"></label>
                                                        <input asp-for="MemberHomePhone" class="form-control" type="text" />
                                                        <span asp-validation-for="MemberHomePhone" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>




                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="CardNo"></label>
                                                        <input asp-for="CardNo" class="form-control" type="text" />
                                                        <span asp-validation-for="CardNo" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="NID"></label>
                                                        <input asp-for="NID" class="form-control" type="text" />
                                                        <span asp-validation-for="NID" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>



                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        @Html.LabelFor(x => x.MarketId)
                                                        @Html.DropDownListFor(x => x.MarketId,
                                                                                                                                                                 new SelectList((IEnumerable<SelectListItem>)ViewBag.Market, "Value", "Text"), "Select Market", htmlAttributes: new { @class = "form-control", id = "MarketId" })
                                                        @Html.ValidationMessageFor(x => x.MarketId, "", new { @class = "text-danger" })
                                                    </div>

                                                    <div class="col-md-6 col-12 ">
                                                        @Html.LabelFor(x => x.ShopId)
                                                        @Html.DropDownListFor(x => x.ShopId, new SelectList((IEnumerable<SelectListItem>)ViewBag.Shop, "Value", "Text"), "Select Shop", htmlAttributes: new { @class = "form-control", id = "Shop" })
                                                        @Html.ValidationMessageFor(x => x.ShopId, "", new { @class = "text-danger" })
                                                    </div>
                                                </div>


                                            </div>


                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <label asp-for="MarketName"></label>
                                                        <input asp-for="MarketName" class="form-control" type="text" />
                                                        <span asp-validation-for="MarketName" class="text-danger"></span>

                                                    </div>


                                                    <div class="col-md-6 col-12 ">
                                                        <div class="form-group">


                                                            <label asp-for="ShopNameEng"></label>
                                                            <input asp-for="ShopNameEng" class="form-control" type="text" />
                                                            <span asp-validation-for="ShopNameEng" class="text-danger"></span>

                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="row">

                                                    <div class="col-md-6 col-12 ">
                                                        <div class="form-group">


                                                            <label asp-for="BusinessAddressBng"></label>
                                                            <input asp-for="HoldingNoBng" class="form-control" />
                                                            <input asp-for="BusinessAddressBng" class="form-control" />


                                                            <span asp-validation-for="BusinessAddressBng" class="text-danger"></span>

                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-12 ">
                                                        <div class="form-group">


                                                            <label asp-for="BusinessAddressEng"></label>
                                                            <input asp-for="HoldingNoEng" class="form-control" type="text" />
                                                            <input asp-for="BusinessAddressEng" class="form-control" type="text" />

                                                            <span asp-validation-for="BusinessAddressEng" class="text-danger"></span>

                                                        </div>
                                                    </div>
                     

                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-4 col-12 ">
                                                        <div class="form-group">


                                                            <label asp-for="FixedAddress"></label>
                                                            <input asp-for="FixedAddress" class="form-control" type="text" />
                                                            <span asp-validation-for="FixedAddress" class="text-danger"></span>

                                                        </div>
                                                    </div>

                                                    <div class="col-md-4 col-12 ">
                                                        <label asp-for="isActive"></label>
                                                        <input asp-for="isActive" class="form-control" type="checkbox" />
                                                        <span asp-validation-for="isActive" class="text-danger"></span>
                                                    </div>


                                                    <div class="col-md-4 col-12 ">
                                                        @Html.LabelFor(x => x.MemberStatusId)
                                                        @Html.DropDownListFor(x => x.MemberStatusId, new SelectList((IEnumerable<SelectListItem>)ViewBag.MemberStatus, "Value", "Text"), "Select Status", htmlAttributes: new { @class = "form-control", id = "MemberStatus" })
                                                        @Html.ValidationMessageFor(x => x.MemberStatusId, "", new { @class = "text-danger" })
                                                    </div>
                                                </div>
                                            </div>




                                        </div>

                                    </div>



                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End PAge Content -->
            </div>

                @section scripts {
                    <script type="text/javascript">
                        $(document).ready(function () {


                        });

                        function PreviewImage() {
                            var oFReader = new FileReader();
                            oFReader.readAsDataURL(document.getElementById("FileUpload").files[0]);

                            oFReader.onload = function (oFREvent) {
                                document.getElementById("UploadFile").src = oFREvent.target.result;
                            };
                        };

                        $("#target :input").prop("disabled", true);
                    </script>
                }







            </div>




    </form>
</body>
</html>

