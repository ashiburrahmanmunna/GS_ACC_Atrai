@using Atrai.Data.Interfaces
@inject IStoreSettingRepository _storeSettingRepository
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor Accessor

@{
    var comid=Accessor.HttpContext.Session.GetInt32("ComId");
    var storesettings = _storeSettingRepository.All(false)
               .Where(x => x.ComId == comid).FirstOrDefault().Logo;
}
<img id="company-image" style="width:10%" class="d-none" src="../@storesettings" />
