                       alert("Failed");

                   }
               });
   }
        $("#saveCurrencyButton").click(function () {
            SaveCurrency();
        });
        function EditCurrencyRate(Id, CurrencyShortName, CurrencyRate,AsOf) {
            $("#Id").val(Id);
            $(".rate").text(CurrencyShortName);
            $("#CurrencyRateText").text(CurrencyShortName);
            $("#CurrencyRate").val(CurrencyRate);

            var formattedDate;

            if (!AsOf) {
                var today = new Date();
                formattedDate = today.toLocaleDateString('en-GB', { day: '2-digit', month: 'short', year: 'numeric' });
                $("#start-date").val(formattedDate);
            } else {
                // Assuming AsOf is a date string like '2024-01-09T00:00:00'
                var parsedDate = new Date(AsOf);
                var formattedDate = parsedDate.toLocaleDateString('en-GB', { day: '2-digit', month: 'short', year: 'numeric' });

                $("#start-date").val(formattedDate);

            }


            $("#exampleModal").modal("show");
        }

